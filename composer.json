{
  "name": "ggbb/api-platform-elasticsearch-integration",
  "description": "The permissions system for users in symfony",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/yaml": "6.3.*",
    "api-platform/core": "3.2.*",
    "doctrine/orm": "^2.15",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Ggbb\\ApiPlatformElasticsearchIntegrationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ggbb\\ApiPlatformElasticsearchIntegrationBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "ggbb",
      "email": "devggbb@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
